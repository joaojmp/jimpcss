{
  "name": "jimpcss",
  "version": "1.0.2",
  "description": "A small CSS package",
  "scripts": {
    "dev": "sass src/jimpcss.scss:build/jimpcss.css",
    "watch": "sass --watch --update src/jimpcss.scss:build/jimpcss.css",
    "prod": "sass --style=compressed src/jimpcss.scss:build/jimpcss.min.css"
  },
  "files": [
    "build/",
    "examples/"
  ],
  "author": "Jimp <joaojmp1997@hotmail.com> (https://jimps.com.br/)",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "sass": "^1.56.1"
  }
}
